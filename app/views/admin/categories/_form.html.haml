= simple_form_for [:admin, @category], html: { class: 'form-horizontal' } do |f|
  - if @category.persisted?
    = f.input :created_at, as: :string, input_html: { disabled: true }
    = f.input :updated_at, as: :string, input_html: { disabled: true }
  = f.input :name
  = f.input :slug
  = f.input :meta_description
  = f.input :matchers
  = f.button :wrapped, cancel: admin_categories_path